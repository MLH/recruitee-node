{
  "env": {
    "mocha": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [
    {
      "files": ["*.test.js", "*.spec.js"],
      "rules": {
        "no-unused-expressions": "off"
      }
    }
  ],
  "plugins": ["filenames"],
  "rules": {
    "filenames/match-exported": 2
  }
}
